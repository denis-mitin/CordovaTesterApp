<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.denistester" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TesterApp</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="deployment-target" value="7.0" />
    </platform>
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="android-minSdkVersion" value="15" />
    </platform>
    <plugin name="cordova-plugin-facebook4" spec="https://github.com/gigya/cordova-plugin-facebook4">
        <variable name="APP_ID" value="176663209146906" />
        <variable name="APP_NAME" value="DenisTester" />
    </plugin>
    <plugin name="cordova-plugin-googleplus" spec="https://github.com/gigya/cordova-plugin-googleplus">
        <variable name="REVERSED_CLIENT_ID" value="670333208366-qqth6mmsv94vq6cdvvnhkk0a6r1a1qh2.apps.googleusercontent.com" />
    </plugin>
    <plugin name="cordova-plugin-gigya" spec="cordova-plugin-gigya">
        <variable name="GOOGLE_IOS_CLIENT_ID" value="670333208366-rh7m29vtufhu0g59nvusao18l57l6lur.apps.googleusercontent.com" />
    </plugin>
</widget>
